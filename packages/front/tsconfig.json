{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "incremental": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "paths": {
      "#modules/*": ["modules/*"],
      "#musics/*": ["modules/musics/*"],
      "#shared/*": ["../shared/build/*"],
      "#sharedSrc/*": [
        // quitar en producci√≥n
        "../shared/src/*"
      ],
      "#styles/*": ["styles/*"],
      "#uikit/*": ["modules/ui-kit/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": false,
    "strictNullChecks": true
  },
  "exclude": ["node_modules"],
  "include": ["**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "next-env.d.ts"]
}
