{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "build/server.js",
  "scripts": {
    "start": "NODE_ENV=production node -r ts-node/register/transpile-only -r tsconfig-paths/register build/server.js",
    "start:watch": "NODE_ENV=development nodemon",
    "prebuild": "npm run clean:build",
    "build": "npm run build:raw",
    "build:raw": "tsc -p tsconfig-build.json",
    "postbuild": "ln -s ../.env build/.env",
    "clean:build": "rm -rf build",
    "clean:modules": "rm -rf node_modules",
    "test": "NODE_ENV=test jest --maxWorkers=1",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "yarn test -- --coverage"
  },
  "dependencies": {
    "axios": "0.21.1",
    "date-ops": "2.1.0",
    "dotenv": "8.2.0",
    "express": "4.18.2",
    "jsdom": "16.5.1",
    "luxon": "1.26.0",
    "mongoose": "6.9.0",
    "node-schedule": "2.0.0",
    "rand-picker": "1.0.0"
  },
  "devDependencies": {
    "@types/express": "4.17.16",
    "@types/jest-expect-message": "1.1.0",
    "@types/jsdom": "16.2.7",
    "@types/luxon": "1.26.2",
    "@types/node-schedule": "2.1.0",
    "jest-expect-message": "1.1.3",
    "nodemon": "2.0.20",
    "tslib": "2.5.0"
  }
}
